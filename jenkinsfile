pipeline {
    agent any
    stages{
        stage ('Compile Stage'){
            steps{
                echo('Hello world')
            }
           
        }
        stage ('Testing Stage'){
            steps{
                withMaven(maven: 'maven_3.9.6' ){
                    sh 'mvn test'
                
            }
        }
        stage ('Deployment Stage'){
            steps{
                withMaven(maven: 'maven_3.9.6' ){
                    sh 'mvn deploy '
                }
            }
        }
    }
}
}